package sorting.doit;

public class Sorting6_1 {
	
	static void swap(int[] a, int idx1, int idx2) {
		int t= a[idx1];
		a[idx1] = a[idx2];
		a[idx2] = t;
	}
	
	static void bubbleSort(int [] a, int n) {
		for(int i = 0; i < n-1; i++) {
			for(int j = n-1 ; j>i; j--) {
				if(a[j=1] > a[j]) {
					swap(a,j-1,j);
				}
			}
		}
	}

}
