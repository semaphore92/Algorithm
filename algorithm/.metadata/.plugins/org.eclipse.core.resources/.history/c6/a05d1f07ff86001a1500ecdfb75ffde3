	}
	
	public void solution(int[] array , int[][] commands) {
		
		int [] b = new int[j-i];
		int cnt = 0;
		
		for(int s = i-1; i<j-1; i++) {
			b[cnt] = a[s];
			cnt ++;
		}
		
		
		
	}
